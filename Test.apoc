
func _apoc_init() -> i32 { -> main(); }

func $interp foo() -> [256]i16 {
	my_arr :[256]i16 = {0};
	for i := 0; i < 10; ++i {
		my_arr[i] = #i16 i;
	}
	-> my_arr;
}

func main() -> i32
{
	array : = $run foo();
	//array :[6]i32 = {1, 2, 3, 4, 5, 6};

	printf("number %d\n", #i32 array[0]);
	printf("number %d\n", #i32 array[1]);
	printf("number %d\n", #i32 array[2]);
	printf("number %d\n", #i32 array[3]);
	printf("number %d\n", #i32 array[4]);
	
	-> #i32 array[3];
}








