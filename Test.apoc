
func $intrinsic var_arg_start(list:** Any);

func test_var_args(a: i32, ...) -> i32
{
	list:* Any;
	var_arg_start(@list);
	if(list[0].type.kind == Type_Type.T_INTEGER)
		-> *#*i32 list[0].value;
	-> a;
}


func _apoc_init() -> i32
{
	printf("%d", test_var_args(10, 34));
	-> 0;
}

