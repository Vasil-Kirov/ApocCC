
String :: $import "Standard/String.apoc"


fn main() -> i32
{
	to_print := Printable_Struct { 10, 20 };
	String.register_printable(print_printable_struct, "Printable_Struct");

	String.print("works? %", to_print);
	-> 0;
}

fn print_printable_struct(the_struct: *void) -> string {
	casted := #*Printable_Struct the_struct;
	-> format_args("Printable Struct:\nnum1: %\nnum2: %", casted.num1, casted.num2);
}

struct Printable_Struct {
	num1: i32;
	num2: i32;
}

