
SOME_NUM :: 10 + 2;

func add(a: i32, b: i32) -> i32 {
	-> a + b;
}

func _apoc_init() -> i32 { -> main(); }

func $interp foo() -> [256]u8 {
	my_arr :[256]u8 = {0};
	for i := 'A'; i <= 'Z'; ++i {
		my_arr[i - 'A'] = #u8 i;
	}
	-> my_arr;
}

func main() -> i32
{
	array :[256]u8 = $run foo();
	for i := 0; i <= 'Z' - 'A'; ++i {
		array[i] = #u8 (array[i] + 'A');
		printf("%d", #i32 array[i]);
	}

	-> 0;
}

