[FATAL] Unknown linker ../libs/*.lib.
Options:
	LINK.EXE
	LD
	WASM-LD

apoc [source files/args]

args:
    --target [platform]
        x64 (default)
	x32
	wasm
   --define DEFINE_NAME=VALUE
   --optimize [level]
       none (default)
       some
       max
    --linker [linker]
        link.exe (windows default)
	ld (linux default)
	wasm-ld (wasm default)
    --link [linker arguments]
        linker dependent
    --dump-symbols
    --debug
    --out [output file name]
    --no-link
    --backend [backend]
        llvm
	custom
    --include [path]

