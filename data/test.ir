; ModuleID = 'Test.apoc'
source_filename = "Test.apoc"
target datalayout = "e-m:w-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128"
target triple = "x86_64-pc-windows-msvc"

define double @add_nums(float %a, float %b) {
entry:
  %ret_val = alloca double, align 8
  %b2 = alloca float, align 4
  %a1 = alloca float, align 4
  store float %a, ptr %a1, align 4
  store float %b, ptr %b2, align 4
  %0 = load float, ptr %a1, align 4
  %1 = load float, ptr %b2, align 4
  %2 = fadd float %0, %1
  %cast = fpext float %2 to double
  store double %cast, ptr %ret_val, align 8
  %3 = load double, ptr %ret_val, align 8
  ret double %3
}
